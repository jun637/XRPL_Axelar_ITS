    "payment": "ts-node src/xrpl/payment.ts",
    "call-contract": "ts-node src/xrpl/call-contract.ts",
    "broadcast": "ts-node src/xrpl/broadcast.ts",
    "decode-address": "ts-node src/xrpl/decode-address.ts",
    "decode-tx-blob": "ts-node src/xrpl/decode-tx-blob.ts",
    "trust-set": "ts-node src/xrpl/trust-set.ts",
    "ticket-create": "ts-node src/xrpl/ticket-create.ts",
    "account-set": "ts-node src/xrpl/account-set.ts",
    "rotate-signers": "ts-node src/xrpl/rotate-signers.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelarnetwork/axelar-contract-deployments.git"
  },
  "keywords": [
    "axelar",
