    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.57.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "hardhat": "~2.19.5",
    "husky": "^9.1.7",
    "jszip": "^3.10.1",
    "mocha": "^10.2.0",
